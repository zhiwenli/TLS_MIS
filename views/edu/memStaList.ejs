<% include ../header %>
<div class="right_content">            

    <h2><%= menu.title %></h2>
    <%- include ../tips %>
    
    <table id="rounded-corner" summary="2007 Major IT Companies' Profit">
        <thead>
        <tr>
            <th scope="col" class="rounded-company">编号</th>
            <th scope="col" class="rounded">姓名</th>
            <th scope="col" class="rounded">邮箱</th>
            <th scope="col" class="rounded">对方状态</th>
        </tr>
        </thead>
    <tfoot>
         <tr>
             <!-- <td colspan="6" class="rounded-foot-left"><em> </em></td>
             <td class="rounded-foot-right">&nbsp;</td> -->
         </tr>
     </tfoot>

     <tbody>

        <% for(var i = 0; i < rows.length; i++) { %>
         <tr>
            <td><%= i+1 %></td>
            <td><%= rows[i]["Mem_Name"] %></td>
            <td><%= rows[i]["Mem_Email"] %></td>
            <td>
                <% if(rows[i]["Mem_Sta"] == 3) { %>
                <img src="/images/in/images/warning.png" alt="" title="待确认" border="0" height=16 width=16 />
                <% } else if(rows[i]["Mem_Sta"] == 1 ) { %>
                <img src="/images/in/images/valid.png" alt="" title="已同意" border="0" height=16 width=16 />
                <% } else if(rows[i]["Mem_Sta"] == 2) { %>
                <img src="/images/in/images/error.png" alt="" title="已拒绝" border="0" height=16 width=16 />
                <% } else if(rows[i]["Mem_Sta"] == 4) { %>
                <img src="/images/in/images/trash.png" alt="" title="已失效" border="0" height=16 width=16 />
                <% } else { %>
                <%= "异常" %>
                <% } %>
            </td>

             <!-- <td><a href="#" class="ask"><img src="/images/in/images/trash.png" alt="" title="" border="0" /></a></td> -->
         </tr>
        <% } %>

     </tbody>
 </table>

<a href="/edu/subEdu?srid=<%= rows[0]["SR_Id"] %>" class="bt_red" onclick="skipCfm()"><span class="bt_red_lft"></span><strong>提交活动申请</strong><span class="bt_red_r"></span></a>

<a href="/edu/eduRep?srid=<%= rows[0]["SR_Id"] %>" class="bt_green" onclick="subChk('返回修改活动信息并重新编辑邀请成员')"><span class="bt_green_lft"></span><strong>修改活动信息</strong><span class="bt_green_r"></span></a>

 <!-- <a href="#" class="bt_red"><span class="bt_red_lft"></span><strong>Delete items</strong><span class="bt_red_r"></span></a>  -->

<p></p>
 <div class="pagination">
    <% if(isFirstPage){ %>
    <span class="disabled">上一页</span>
    <% } else { %>
    <a href="?page=<%= (page - 1) %>">上一页</a>
    <% } %>

    <% if(page - 5 > 0) { %><a href="?page=<%= (page - 5) %>"><%= page-5 %></a><% } %>
    <% if(page - 4 > 0) { %><a href="?page=<%= (page - 4) %>"><%= page-4 %></a><% } %>
    <% if(page - 3 > 0) { %><a href="?page=<%= (page - 3) %>"><%= page-3 %></a><% } %>
    <% if(page - 2 > 0) { %><a href="?page=<%= (page - 2) %>"><%= page-2 %></a><% } %>
    <% if(page - 1 > 0) { %><a href="?page=<%= (page - 1) %>"><%= page-1 %></a><% } %>

    <a href=""><font color="balck"><%= page %>/<%= totalPage %></font></a>

    <% if(parseInt(page) + 1 <= totalPage) { %><a href="?page=<%= parseInt(page) + 1 %>"><%= parseInt(page) + 1 %></a><% } %>
    <% if(parseInt(page) + 2 <= totalPage) { %><a href="?page=<%= parseInt(page) + 2 %>"><%= parseInt(page) + 2 %></a><% } %>
    <% if(parseInt(page) + 3 <= totalPage) { %><a href="?page=<%= parseInt(page) + 3 %>"><%= parseInt(page) + 3 %></a><% } %>
    <% if(parseInt(page) + 4 <= totalPage) { %><a href="?page=<%= parseInt(page) + 4 %>"><%= parseInt(page) + 4 %></a><% } %>
    <% if(parseInt(page) + 5 <= totalPage) { %><a href="?page=<%= parseInt(page) + 5 %>"><%= parseInt(page) + 5 %></a><% } %>

    <% if(isLastPage){ %>
    <span class="disabled">下一页</span>
    <% } else { %>
    <a href="?page=<%= parseInt(page) + 1 %>">下一页</a>
    <% } %>
</div> 

</div>
     <!-- end of right content-->
        
<% include ../footer %>
